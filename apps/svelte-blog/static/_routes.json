{
  "version": 1,
  "description": "Cloudflare Pages routing configuration for optimal static/dynamic separation",
  "include": ["/api/*", "/category/*/*", "/tag/*/*"],
  "exclude": [
    "/post/*",
    "/*.[js|css|svg|png|jpg|jpeg|webp|ico|txt|xml]",
    "/category/*/1",
    "/tag/*/1",
    "/"
  ]
}
